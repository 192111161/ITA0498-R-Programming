# 5×4 matrix filled by rows
mat1 <- matrix(1:20, nrow=5, ncol=4, byrow=TRUE)
print(mat1)
# 3×3 matrix with labels, filled by rows
mat2 <- matrix(1:9, nrow=3, ncol=3, byrow=TRUE, dimnames=list(c("A", "B", "C"), c("X", "Y", "Z")))
print(mat2)
# 2×2 matrix with labels, filled by columns
mat3 <- matrix(1:4, nrow=2, ncol=2, byrow=FALSE, dimnames=list(c("Row1", "Row2"), c("Col1", "Col2")))
print(mat3)
